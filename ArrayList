import java.util.ArrayList;

public class Main {
  public static void main(String[] args) {
    ArrayList<String> cars = new ArrayList<String>();
    cars.add("Volvo");
    cars.add("BMW");
    cars.add("Ford");
    cars.add("Mazda");
    System.out.println(cars);
  }
}


cars.get(0); // Volvo
cars.set(0, "Opel");  // new 0 : Volvo
cars.remove(0);   // 0. elemani siler
cars.clear();  // hepsini siler
